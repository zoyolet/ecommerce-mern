<h1>Product page</h1>
<h1>Product Lists</h1>
    <div>
        {{#each items}}
            <div>
              <a target="_blank" href="/product?productId={{this.id}}">
                     <img style='width: 150x;height:150px; object-fit: contain'  src="data:image/{{this.img.contentType}};base64,{{this.img.data}}"/>
              </a>
                <div>
                    <h5>{{this.name }}</h5>
                </div>
            </div>
        {{/each}}
    </div>